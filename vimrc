set autoindent
syntax on
set background=dark
set tabstop=2
set shiftwidth=2
set expandtab
filetype on
filetype plugin indent on

" Only on Mac ...
set backspace=indent,eol,start


" highlight line parts over 80 chars
" highlight OverLength ctermbg=red ctermfg=white guibg=#592929
" match OverLength /\%81v.\+/
